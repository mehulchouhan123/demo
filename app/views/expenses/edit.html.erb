<%=form_with model:@expense do |form|%>
<%=form.label "Title:"%>
<%=form.text_field:title%><br>
<%=form.label "price:"%>
<%=form.text_field:price%><br>
<%= form.date_field :date, :value => @date  %>
<%=form.label "Type:"%>
<%= form.select :type_of_expenses, ["Education","Food","Housing","Transporation","Clothing"], autofocus: true%><br>

<%=form.submit :submit%>
<%end%>