<%=form_with model:@expense do |form|%>
<%=form.label "Title:"%>
<%=form.text_field:title%><br>
<%=form.label "price:"%>
<%=form.text_field:price%><br>
<%=form.label "Date:"%>
<%= form.date_field :date %><br>
<%=form.label "Type:"%>
<%= form.select :type_of_expenses, ["Education", "clothes", "food"] %><br>

<%=form.submit :submit%>
<%end%>